{# empty Twig template #}
{% block head %}
<!DOCTYPE html>
<html lang="{{ grav.language.getActive ?: theme_config.default_lang }}">
<head>

    <meta charset="utf-8" />
    <title>{% if header.title %}{{ header.title|e('html') }} | {% endif %}{{ site.title|e('html') }}</title>

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% include 'partials/metadata.html.twig' %}

    <link rel="icon" type="image/png" href="{{ url('image://row.png') }}" />
    <link rel="canonical" href="{{ page.url(true, true) }}" />

    {% block stylesheets %}
        {% do assets.addCss('https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css', 100) %}
{#        {% do assets.addCss('https://unpkg.com/purecss@1.0.0/build/pure-min.css', 100) %}#}
        {% do assets.addCss('https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css', {'priority':102, 'pipeline': false}) %}
{#        {% do assets.addCss('theme://css/custom.css', 98) %}#}
        {% do assets.addCss('theme://css-compiled/theme.css', 98) %}
        {% do assets.addCss('theme://css-compiled/bootstrap.min.css', 98) %}
    {% endblock %}
    {{ assets.css() }}

    {% block javascripts %}
        {% do assets.addJs('jquery', 100) %}
        {% do assets.addJs('https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js', {'priority':102, 'group':'bottom'}) %}
        {% do assets.addJs('theme://js/modern-business.js', {'priority':102, 'group':'bottom'}) %}
        {% do assets.addJs('theme://js/owl.carousel.min.js', {'priority':102 , 'group':'bottom'}) %}
    {% endblock %}
    {{ assets.js() }}


</head>
{% endblock head%}
